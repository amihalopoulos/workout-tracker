<div class="rounds_form">
  <%= form_for @round, url: user_workout_exercise_rounds_path, remote: true do |f| %>
    <h2>Add Set</h2>
    <%= f.label :reps, "Number of Reps: " %>
    <%= f.text_field :reps %> <br>
    <%= f.label :weight, "Weight Used:" %>
    <%= f.text_field :weight %> <br>
    <%= f.submit 'Submit' %>
  <% end %>
</div>